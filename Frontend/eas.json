{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "@secret:EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_KEY": "@secret:EXPO_PUBLIC_SUPABASE_KEY",
        "GOOGLE_MAPS_API_KEY": "@secret:GOOGLE_MAPS_API_KEY",
        "API_URL": "@secret:API_URL"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "@secret:EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_KEY": "@secret:EXPO_PUBLIC_SUPABASE_KEY",
        "GOOGLE_MAPS_API_KEY": "@secret:GOOGLE_MAPS_API_KEY",
        "API_URL": "@secret:API_URL"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "@secret:EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_KEY": "@secret:EXPO_PUBLIC_SUPABASE_KEY",
        "GOOGLE_MAPS_API_KEY": "@secret:GOOGLE_MAPS_API_KEY",
        "API_URL": "@secret:API_URL"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
